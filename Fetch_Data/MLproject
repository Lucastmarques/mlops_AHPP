name: fetch_data
conda_env: conda_fetch.yml

entry_points:
  main:
    parameters:
      input_url:
        description: "Google Drive URL to download dataset (Allowed formats: .csv, .gz, .zip)"
        type: str
      artifact_name:
        description: Name for the W&B artifact that will be created
        type: str
      artifact_type:
        description: Type of the artifact to create
        type: str
      artifact_description:
        description: Description for the artifact
        type: str
        default: Sample Artifact

    command: >-
      python fetch.py --input_url {input_url} --artifact_name {artifact_name} --artifact_type {artifact_type} --artifact_description {artifact_description}
                      